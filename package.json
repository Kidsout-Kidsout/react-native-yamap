{
  "name": "@kidsout-kidsout/react-native-yamap",
  "version": "5.0.5",
  "description": "Yandex.Maps SDK for React Native",
  "scripts": {
    "example": "yarn workspace react-native-kidsout-yamap-example",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "build": "bob build",
    "typecheck": "tsc",
    "test": "jest",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,ts,tsx}\""
  },
  "types": "./lib/typescript/src/index.d.ts",
  "source": "./src/index",
  "react-native": "./src/index",
  "main": "./lib/module/index.js",
  "exports": {
    ".": {
      "types": "./lib/typescript/src/index.d.ts",
      "source": "./src/index.ts",
      "react-native": "./src/index.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],

  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://dev.kidsout.ru.git"
  },
  "author": "Vyrtsev Mikhail <dev@kidsout.ru> (https://dev.kidsout.ru)",
  "license": "MIT",
  "bugs": {
    "url": "https://dev.kidsout.ru/issues"
  },
  "homepage": "https://dev.kidsout.ru#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native-community/cli": "^20.1.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-builder-bob": "^0.40.13",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNYamapSpec",
    "type": "all",
    "jsSrcsDir": "src",
    "ios": {
      "modules": {
        "Yamap": {
          "className": "KDSTYamap",
          "unstableRequiresMainQueueSetup": true
        },
        "YamapGeocode": {
          "className": "KDSTYamapGeocode",
          "unstableRequiresMainQueueSetup": true
        },
        "YamapSuggests": {
          "className": "KDSTYamapSuggests",
          "unstableRequiresMainQueueSetup": true
        }
      },
      "components": {
        "YamapView": {
          "className": "KDSTYamapView"
        },
        "YamapCircleView": {
          "className": "KDSTYamapCircleView"
        },
        "YamapMarkerView": {
          "className": "KDSTYamapMarkerView"
        },
        "YamapPolygonView": {
          "className": "KDSTYamapPolygonView"
        },
        "YamapClustersView": {
          "className": "KDSTYamapClustersView"
        }
      }
    },
    "android": {
      "javaPackageName": "ru.kidsout.yamap"
    }
  },
  "create-react-native-library": {
    "type": "turbo-module",
    "languages": "kotlin-objc",
    "tools": [
      "jest",
      "eslint"
    ],
    "version": "0.56.1"
  }
}
